<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.loginwindow">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:windowSoftInputMode="adjustPan"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.CadastroActivity"></activity>
        <activity android:name=".activity.MainActivity"></activity>
        <activity android:name=".activity.OsActivity"></activity>
        <activity
            android:name=".activity.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.VeiculoActivity"
            android:windowSoftInputMode="adjustResize"
            />
        <activity android:name=".activity.MainActivityNova" />
        <activity android:name=".activity.ScreenSlidePagerActivity" />
        <activity android:name=".activity.EncargoActivity"/>
        <activity android:name=".activity.EncargoPagerActivity"/>
        <activity android:name=".activity.VeiculoTesteRecycler"/>
    </application>

</manifest>